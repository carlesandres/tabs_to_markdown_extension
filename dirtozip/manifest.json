{
  "manifest_version": 2,

  "name": "TabsToMarkdown",
  "description": "Generate a markdown file with links to all your open tabs",
  "version": "1.3",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "64": "icon.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
